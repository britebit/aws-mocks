{
  "name": "@app-press/aws-mocks",
  "version": "1.0.0",
  "description": "Mocks of key AWS services for testing and local development",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint 'lib/**/*.js' --ignore-path .gitignore",
    "pretest": "npm run lint -s",
    "test": "mocha --compilers js:babel-core/register",
    "watch:test": "npm run test -s -- --watch -R min",
    "precoverage": "npm run lint -s",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover _mocha",
    "prebuild": "npm run clean -s && npm run test -s",
    "build": "babel lib --out-dir dist -s inline",
    "prepublish": "npm run build -s"
  },
  "author": "Chris Henderson <chris@app-press.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "~6.6.5",
    "babel-core": "~6.7.4",
    "babel-eslint": "~6.0.0-beta.6",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-2": "~6.5.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "eslint": "~2.4.0",
    "eslint-plugin-babel": "~3.1.0",
    "isparta": "~4.0.0",
    "mocha": "~2.4.5",
    "rimraf": "~2.5.2"
  },
  "dependencies": {
    "async": "~2.0.0-rc.2",
    "mongodb": "~2.1.11",
    "mongodb-queue": "github:cwhenderson20/mongodb-queue"
  }
}
